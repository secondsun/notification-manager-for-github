<StackPane  xmlns:fx="http://javafx.com/fxml">
    <BorderPane>

        <top>
            <VBox spacing="20">
                <JFXToolbar>
                    <leftItems>
                        <JFXRippler  fx:id="titleBurgerContainer" maskType="CIRCLE" style="-fx-ripple-color:WHITE;">
                            <StackPane >
                                <JFXHamburger fx:id="titleBurger">
                                    <HamburgerBackArrowBasicTransition />
                                </JFXHamburger>
                            </StackPane>
                        </JFXRippler>
                        <Label fx:id="fragmentTitle" >Notifications</Label>
                    </leftItems>
                </JFXToolbar>
            </VBox>
        </top>

        <!-- Content Area -->
        <center>
            <StackPane fx:id="root"></StackPane>
        </center>
    </BorderPane>

</StackPane>
